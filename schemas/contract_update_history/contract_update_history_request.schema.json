{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Contract Update History Request",
  "description": "Request for contract update history.",
  "type": "object",
  "properties": {
    "contract_update_history": {
      "type": "integer",
      "description": "Must be `1`",
      "enum": [1]
    },
    "contract_id": {
      "type": "integer",
      "description": "Internal unique contract identifier."
    },
    "limit": {
      "type": "number",
      "description": "Maximum number of historical updates to receive."
    },
    "passthrough": {
      "type": "object",
      "description": "Used to pass data through the websocket, which may be retrieved via the `echo_req` output field."
    },
    "req_id": {
      "type": "integer",
      "description": "Used to map request to response."
    }
  },
  "required": ["contract_update_history", "contract_id"],
  "additionalProperties": false
}

